JINA_API_KEY =
MISTRAL_API_KEY =
COHERE_API_KEY =
SILICONCLOUD_API_KEY =
OPENAI_API_KEY =
ALIBABA_API_KEY = 



# VDB_BACKEND 配置
VDB_BACKEND=FAISS
VDB_TOPK=3

# FAISS_BACKEND 配置
# ======== 基础配置 ========
FAISS_INDEX_TYPE=IndexHNSWFlat  # 可选类型:
                               # 基础: IndexFlatL2, IndexFlatIP
                               # 图: IndexHNSWFlat
                               # IVF: IndexIVFFlat, IndexIVFPQ, IndexIVFScalarQuantizer
                               # 其他: IndexLSH, IndexPQ, IndexScalarQuantizer
                               # 包装: IndexIDMap, IndexRefineFlat

# ======== HNSW 参数 ========
HNSW_M=32             # 连接数 (典型值16-64)
HNSW_EF_CONSTRUCTION=200  # 构建时的搜索深度
HNSW_EF_SEARCH=100    # 搜索时的深度 (可选)

# ======== IVF 参数 ========
IVF_NLIST=100         # 聚类中心数 (建议sqrt(N)到4*sqrt(N))
IVF_NPROBE=10         # 搜索时探查的聚类数 (1-nlist)
IVF_METRIC=L2         # 距离度量: L2 或 IP

# ======== 乘积量化参数 ========
PQ_M=8                # 子量化器数量 (需能被DIM整除)
PQ_NBITS=8            # 每子量化器的比特数 (通常4-8)

# ======== LSH 参数 ========
LSH_NBITS=512         # 哈希位数 (典型值256-1024)
LSH_ROTATE_DATA=True  # 是否旋转数据
LSH_TRAIN_THRESHOLDS=False  # 是否训练阈值

# ======== 标量量化参数 ========
SQ_TYPE=QT_8bit       # 量化类型: 
                      # QT_4bit, QT_4bit_uniform
                      # QT_8bit, QT_8bit_uniform
                      # QT_fp16, QT_bf16
SQ_METRIC=L2          # 距离度量: L2 或 IP

# ======== 精炼索引参数 ========
REFINE_K_FACTOR=1.0   # 精检索时的扩展系数

# ======== 基础索引配置 ========
FAISS_BASE_INDEX_TYPE=IndexFlatL2  # 用于IndexIDMap/IndexRefineFlat的基础索引